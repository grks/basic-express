[{
  "group": "*",
  "permissions": [{
    "resource": "*",
    "methods": "*",
    "action": "deny"
  }]
},
 {
   "group": "anonymous",
   "permissions": [{
     "resource": "token",
     "methods": [
       "POST"
     ],
     "action": "allow"
   }]
 },
 {
   "group": "admin",
   "permissions": [{
     "resource": "*",
     "methods": "*",
     "action": "allow"
   }]
 }
]
